{
    "env": {
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": [
            "warning",
            "unix"
        ],
        "semi": [
            "warning",
            "always"
        ],
        "camelcase": 1,
        "eol-last": "warn"
    }
}