{
    "env": {
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": 1,
        "semi": 1,
        "camelcase": 1,
        "eol-last": 1,
        "no-undef": 0,
        "no-useless-catch": 0,
        "no-empty": 0,
        "no-case-declarations": 0
    }
}