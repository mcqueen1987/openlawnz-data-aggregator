    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch cases aggregation",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/getCases.js",
            "args": [
                "--env=local",
                "--datasource=moj"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch legislation aggregation",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/getLegislation.js",
            "args": [
                "--env=local",
                "--datasource=pco"
            ]
        }
    ]
}